{
    "canisters": {
        "product_category_manager": {
            "type": "azle",
            "main": "src/ProductCategoryManager.ts",
            "declarations": {
                "output": "declarations/product_category_manager",
                "node_compatibility": true
            },
            "custom": {
                "experimental": true,
                "env": ["CANISTER_ID_DELEGATE_MANAGER"]
            }
        },
        "material_manager": {
            "type": "azle",
            "main": "src/MaterialManager.ts",
            "declarations": {
                "output": "declarations/material_manager",
                "node_compatibility": true
            },
            "custom": {
                "env": ["CANISTER_ID_PRODUCT_CATEGORY_MANAGER"]
            }
        },
        "delegate_manager": {
            "type": "azle",
            "main": "src/DelegateManager.ts",
            "init_arg_file": "./init/delegate_manager_init.did",
            "declarations": {
                "output": "declarations/delegate_manager",
                "node_compatibility": true
            },
            "custom": {
                "experimental": true
            }
        },
        "internet_identity": {
            "type": "custom",
            "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
            "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
            "declarations": {
                "output": "declarations/internet_identity",
                "node_compatibility": true
            }
        },
        "ic_siwe_provider": {
            "candid": "https://github.com/kristoferlund/ic-siwe/releases/download/v0.0.6/ic_siwe_provider.did",
            "type": "custom",
            "wasm": "https://github.com/kristoferlund/ic-siwe/releases/download/v0.0.6/ic_siwe_provider.wasm.gz",
            "init_arg_file": "./init/siwe_init.did",
            "declarations": {
                "output": "declarations/ic_siwe_provider",
                "node_compatibility": true
            }
        }
    },
    "output_env_file": ".env",
    "version": 1
}
